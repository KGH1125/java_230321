package play;

public class Play {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		RanMaker ran = new RanMaker();
		//ran.MakeRan = all개 중에 win개 당첨
		int all= 10;
		int win= 2;
		
		System.out.printf("%f확률\n",(double)(all/win)*100);
		System.out.println(ran.MakeRan(all, win));
	}

}
