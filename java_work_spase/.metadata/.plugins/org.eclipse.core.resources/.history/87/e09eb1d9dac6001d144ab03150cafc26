package play;

public class Play {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		RanMaker ran = new RanMaker();
		//ran.MakeRan = all개 중에 win개 당첨
		int all= 100;
		int win= 2;
		
		System.out.printf("확률 : %.1f\n",(double)(win/all)*10);
		
		
		System.out.println(ran.MakeRan(all, win));
	}

}
