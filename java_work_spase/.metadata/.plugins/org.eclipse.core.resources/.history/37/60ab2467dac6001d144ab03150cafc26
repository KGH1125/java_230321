package play;

public class Play {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		RanMaker ran = new RanMaker();
		//ran.MakeRan = all개 중에 win개 당첨
		for (int i = 0; i < 10; i++) {
			int a = 0;
			for (int j = 0; j < 10; j++) {
				if ((ran.MakeRan(2, 1)) == true) {
					a++;
				}

			}
			System.out.println(a);
		}
	}

}
